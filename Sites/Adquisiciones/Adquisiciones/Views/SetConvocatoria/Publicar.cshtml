<div class="tab-pane" id="10">

    <div class="row first">
        <div class="col-lg-12 text-center ">
            <button class="btn btn-success btn-Genera">Generar convocatoria</button>
            <img class="loaderapp" style="display:none" src="~/Images/ajax-loading.gif" />
        </div>
    </div>

    <div style="display:none" class="row Publica-Container">
        <div class="col-lg-3 text-center ">
            <label>Folio de convocatoria:</label>
            <input disabled type="text" class="form-control Nconv_text" />
        </div>
        <div class="col-lg-3 text-center ">
            <label>Número de solicitud:</label>
            <input disabled type="text" class="form-control Sol_text" />
        </div>
        <div class="col-lg-3 text-center ">
            <label>Folio de publicacion:</label>
            <input type="text" class="form-control txt_folio_conv_pub" placeholder="Folio de publicación" />
        </div>
        <div class="col-lg-3 text-center ">
            <label>Tipo de publicación:</label>
            <select class="form-control cmb_tipo_publicacion">
                <option value="">Seleccione ...</option>
                <option value="compranet">Compranet</option>
                <option value="dof">DOF</option>
                <option value="portal institucional">Portal Institucional</option>
            </select>
        </div>
    </div>

    <div style="display:none" class="row accions">
        <div class="col-lg-12 text-center ">
            <br />

            <button class="btn btn-info" data-toggle="modal" data-target="#myModal"> <i class="fa fa-file-text-o"></i> Ver convocatoria </button>

            <button class="btn btn-success publicar_conv" onclick="Publicar()"> Guardar </button>
        </div>
    </div>

    <!-- Modal -->
    <div id="myModal" class="modal" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content" style="max-height: 700px !important;height: 700px;">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Convocatoria</h4>
                </div>
                <div class="modal-body" style="max-height: 100%;height: 550px;">
                    <iframe style="width:100%; max-height: 100%; height: 100%;" src="https://www.tdx.cat/bitstream/handle/10803/9260/Jorge_Leiva_TesisCompleta.pdf"></iframe>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </div>

        </div>
    </div>

</div>





