@model Dictionary<String, String>


<section class="content-header">
    <br />
    <ol class="breadcrumb">

        @{


            if (Model.Count > 0)
            {
                foreach (KeyValuePair<String, String> entry in Model)
                {
                    if (entry.Key == "-")
                    {
                        <li><a>@entry.Value</a></li>
                    }
                    else
                    {
                        <li><a href="@entry.Key">@entry.Value</a></li>
                    }

                }
            }
            else
            {
                <br />
            }


        }








    </ol>
</section>

