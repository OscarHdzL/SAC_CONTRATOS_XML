
@{

    Layout = null;
}


<div class="box box-default div_deductivas" style="display:none">
    <div class="box-body">
        <div class="row">
            <div class="col-lg-3">
                Porcentaje Máximo de Penalización
                <input type="number" id="PMP" class="form-control Requerido_" />
            </div>
            <div class="col-lg-3">
                Porcentaje Máximo de Deductivas
                <input type="number" id="PMD" class="form-control Requerido_"  />
            </div>
            <div class="col-lg-3">
                Porcentaje de Garantia
                <input type="number" id="PG" class="form-control Requerido_"/>
            </div>
            <div class="col-lg-3">
                Monto de la Garantia
                <input id="MG" class="form-control Requerido_" />
            </div>
        </div>
        <div class="row">
            <div class="col-lg-4">
                Procedimiento
                <select id="cmb_procedimiento" class="form-control Requerido_">
                    <option value="">Seleccione...</option>
                </select>
            </div>
            <div class="col-lg-4">
                Prioridad
                <select id="cmb_prioridad" class="form-control Requerido_" >
                    <option value="">Seleccione...</option>
                </select>
            </div>
            <div class="col-lg-4">
                Estatus del Contrato
                <select id="cmb_estatus" class="form-control Requerido_" >
                    <option value="">Seleccione...</option>
                </select>
            </div>
        </div>
        <div class="row text-right">
            <button class="btn btn-success dadicionales_btn" onclick="validadatosAdiciones()" style="margin-right:10px;margin-top: 30px;"><i class="fa fa-fw fa-check-circle-o"></i>Validar</button>
        </div>
    </div>
</div>

