<div id="Modal_Detalle_Proveedor" class="modal" role="dialog">
    <div class="modal-dialog" style="width: 60%; padding-top: 5%; background-color: transparent;">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Detalle del registro</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-lg-6">
                        <label>Tipo de interlocutor</label>
                        <select class="form-control" id="txt_tipo_interlocutor_det" disabled>
                        </select>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-6">
                        <label>Razón social</label>
                        <input type="text" class="form-control txt_clear" id="txt_razon_social_det" placeholder="Razón social" disabled/>
                    </div>
                    <div class="col-lg-6">
                        <label>RFC</label>
                        <input type="text" class="form-control txt_clear" id="txt_RFC_det" placeholder="RFC" maxlength="12" disabled/>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-4">
                        <label>Representante</label>
                        <input type="text" class="form-control txt_clear" id="txt_representante_det" placeholder="Representante" disabled/>
                    </div>
                    <div class="col-lg-4">
                        <label>Apellido paterno</label>
                        <input type="text" class="form-control txt_clear" id="txt_apellido_paterno_r_det" placeholder="Apellido paterno" disabled/>
                    </div>
                    <div class="col-lg-4">
                        <label>Apellido materno</label>
                        <input type="text" class="form-control txt_clear" id="txt_apellido_materno_r_det" placeholder="Apellido paterno" disabled/>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-4">
                        <label>Ejecutivo de cuenta</label>
                        <input type="text" class="form-control txt_clear" id="txt_ejecutivo_cuenta_det" placeholder="Ejecutivo de cuenta" disabled/>
                    </div>
                    <div class="col-lg-4">
                        <label>Apellido paterno</label>
                        <input type="text" class="form-control txt_clear" id="txt_apellido_paterno_eje_det" placeholder="Apellido paterno" disabled/>
                    </div>
                    <div class="col-lg-4">
                        <label>Apellido materno</label>
                        <input type="text" class="form-control txt_clear" id="txt_apellido_materno_eje_det" placeholder="Apellido paterno" disabled/>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <label>Domicilio fiscal</label>
                        <textarea class="form-control txt_clear" id="txt_Domicilio_fiscal_det" placeholder="Domicilio fiscal" rows="2" style="resize:none" disabled></textarea>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-6">
                        <label>Teléfono</label>
                        <input type="text" class="form-control txt_clear" id="txt_telefono_det" placeholder="Teléfono" disabled/>
                    </div>
                    <div class="col-lg-6">
                        <label>Correo electrónico</label>
                        <input type="text" class="form-control txt_clear" id="txt_correo_electronico_det" placeholder="Correo electrónico" disabled/>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <div align="center">
                    <button type="button" class="btn btn-danger" data-dismiss="modal">Cerrar</button>
                </div>
            </div>
        </div>
    </div>
</div>
