
<input type="hidden" id="id_proveedor" value=""/>
<div id="Modal_Editar_Proveedor" class="modal" role="dialog">
    <div class="modal-dialog" style="width: 60%; padding-top: 5%; background-color: transparent;">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Actualizar interlocutor</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-lg-6">
                        <label>Tipo de interlocutor editado</label>
                        <select class="form-control" id="txt_tipo_interlocutor_upd">
                        </select>
                    </div>
                </div>                           

                <div class="row">
                    <div class="col-lg-6">
                        <label>Razón social</label>
                        <input type="text" class="form-control txt_clear" id="txt_razon_social_upd" placeholder="Razón social" />
                    </div>
                    <div class="col-lg-6">
                        <label>RFC</label>
                        <input type="text" class="form-control txt_clear" id="txt_RFC_upd" placeholder="RFC" maxlength="12" />
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-4">
                        <label>Representante</label>
                        <input type="text" class="form-control txt_clear" id="txt_representante_upd" placeholder="Representante" />
                    </div>
                    <div class="col-lg-4">
                        <label>Apellido paterno</label>
                        <input type="text" class="form-control txt_clear" id="txt_apellido_paterno_r_upd" placeholder="Apellido paterno" />
                    </div>
                    <div class="col-lg-4">
                        <label>Apellido materno</label>
                        <input type="text" class="form-control txt_clear" id="txt_apellido_materno_r_upd" placeholder="Apellido paterno" />
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-4">
                        <label>Ejecutivo de cuenta</label>
                        <input type="text" class="form-control txt_clear" id="txt_ejecutivo_cuenta_upd" placeholder="Ejecutivo de cuenta" />
                    </div>
                    <div class="col-lg-4">
                        <label>Apellido paterno</label>
                        <input type="text" class="form-control txt_clear" id="txt_apellido_paterno_eje_upd" placeholder="Apellido paterno" />
                    </div>
                    <div class="col-lg-4">
                        <label>Apellido materno</label>
                        <input type="text" class="form-control txt_clear" id="txt_apellido_materno_eje_upd" placeholder="Apellido paterno" />
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <label>Domicilio fiscal</label>
                        <textarea class="form-control txt_clear" id="txt_Domicilio_fiscal_upd" placeholder="Domicilio fiscal" rows="2" style="resize:none"></textarea>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-6">
                        <label>Teléfono</label>
                        <input type="text" class="form-control txt_clear" id="txt_telefono_upd" placeholder="Teléfono" />
                    </div>
                    <div class="col-lg-6">
                        <label>Correo electrónico</label>
                        <input type="text" class="form-control txt_clear" id="txt_correo_electronico_upd" placeholder="Correo electrónico" />
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <div align="center">
                    <button type="button" class="btn btn-primary" id="btn_actualizar_p">Actualizar</button>
                    <button type="button" class="btn btn-danger" data-dismiss="modal">Cerrar</button>
                </div>
            </div>
        </div>
    </div>
</div>
