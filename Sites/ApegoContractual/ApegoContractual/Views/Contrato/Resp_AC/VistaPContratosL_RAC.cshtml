<input type="hidden" id="dependenciaContrato" />

<div class="box">
    <div class="box-header with-border">
        <div class="row">
            <div class="col-lg-4">
                <h3 class="box-title">Contratos</h3>
            </div>
            <div class="col-lg-1">
                &nbsp;
            </div>
            <div class="col-lg-2">
                <select class="form-control" id="txt_estatus_contrato" onchange="filtrar_contrato(this.value);">
                    <option value="1" selected>Filtrar contrato</option>
                    <option value="1">Contratos activos</option>
                    <option value="0">Contratos cerrados</option>
                    <option value="2">Todos los contratos</option>
                </select>
            </div>
            <div class="col-lg-1">
                &nbsp;
            </div>
            <div class="col-lg-4">
                <div class="box-tools pull-right">

                    <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip"
                            title="Collapse">
                        <i class="fa fa-minus"></i>
                    </button>
                </div>
            </div>
        </div>
    </div>
    <div class="box-body">
        <table id="tbl_contrato_RAC" class="display" style="width:100%">
            <thead>
                <tr>
                    <th>No.</th>
                    <th>Nombre</th>
                    <th>Inicio</th>
                    <th>Fin</th>
                    <th>Tipo</th>
                    <th>Estatus</th>
                    <th>Acciones</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

<div id="modalVerDetalle" class="modal" role="dialog" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h3 class="modal-title">Detalle de contrato</h3>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-lg-4">
                        <label>ID</label>
                    </div>
                    <div class="col-lg-4">
                        <label>Número</label>
                    </div>
                    <div class="col-lg-4">
                        <label>Objeto</label>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-4">
                        <span id="lblIdContrato">ID</span>
                    </div>
                    <div class="col-lg-4">
                        <span id="lblNumer
                              o">Número</span>
                    </div>
                    <div class="col-lg-4">
                        <span id="lblObjeto">Objeto</span>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
            </div>
        </div>

    </div>
</div>


