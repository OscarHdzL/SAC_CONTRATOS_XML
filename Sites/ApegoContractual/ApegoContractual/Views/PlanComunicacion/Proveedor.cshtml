@model Guid

<input type="hidden" id="idContratoProveedor" value="@Model" />
<input type="hidden" id="idProveedor" value="" />


<div class="box box-default">

    <div class="box-header with-border">
        <h3 class="box-title">Interlocutor</h3>
        <div class="box-tools pull-right">
            <button type="button" onclick="ModalAgregarProveedor();" class="btn btn-box-tool" data-toggle="tooltip"
                    title="Agregar interlocutor">
                Agregar interlocutor
                <i class="fa fa-fw fa-plus-circle"></i>
            </button>

            <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip"
                    title="Collapse">
                <i class="fa fa-minus"></i>
            </button>
        </div>
    </div>
    <div class="box-body">
        <div class="row air" style="margin-top: 1%;">

            <div class="col-lg-12">

                <table id="tbl_Proveedor" class="display" style="width:100%">
                    <thead>
                        <tr>
                            <th>Nombre</th>
                            <th>Ubicación</th>
                            <th>Tipo de audiencia</th>
                            <th>Teléfono</th>
                            <th>Extensión</th>
                            <th>Correo electrónico</th>
                            <th>Acción</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

@*Modal Agregar/Editar*@


<div class="modal" id="ModalProveedor" data-backdrop="static" data-keyboard="false" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document" style="WIDTH: 80%; padding-top: 5%; background-color: transparent;">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" onclick="CerrarModalProveedor();" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                <h3 class="modal-title" id="TitleModalProveedor">Agregar interlocutor</h3>
            </div>
            <div class="modal-body">

                <div class="row">
                    <div class="col-lg-4">
                        <label>Nombre</label>
                        <br />
                        <select name="DropNombreProveedor" id="DropNombreProveedor" class="form-control DropNombreProveedor clean">
                        </select>

                    </div>
                    <div class="col-lg-4">
                        <label>Ubicación</label>
                        <br />
                        <select name="DropUbicacionProveedor" id="DropUbicacionProveedor" class="form-control DropUbicacionProveedor clean">
                        </select>
                    </div>
                    <div class="col-lg-4">
                        <label>Tipo audiencia</label>
                        <br />
                        <select name="DropTipoAudienciaProveedor" id="DropTipoAudienciaProveedor" class="form-control DropTipoAudienciaProveedor clean">
                        </select>
                    </div>
                </div>

                <div class="row">

                    <div class="col-lg-4">
                        <label>Correo electrónico</label>
                        <br />
                        <input type="text" id="CorreoProveedor" disabled class="form-control clean" />
                    </div>
                    <div class="col-lg-4">
                        <label>Teléfono</label>
                        <br />
                        <input type="text" id="TelefonoProveedor" disabled class="form-control clean" />
                    </div>
                    <div class="col-lg-4">
                        <label>Extensión</label>
                        <br />
                        <input type="text" id="ExtensionProveedor" disabled class="form-control clean" />
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <div class="col-lg-12" align="center">
                    <button class="btn btn-primary" id="AddProveedor">Guardar</button>
                    <button class="btn btn-danger" id="Cerrar" onclick="CerrarModalProveedor();">Cancelar</button>
                </div>
            </div>
        </div>
    </div>
</div>