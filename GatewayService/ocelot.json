{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/api/{path}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 44359
        }
      ],
      "UpstreamPathTemplate": "/Servicio/{path}",

      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Put",
        "Delete"
      ],
      "ReRouteIsCaseSensitive": true
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:8080"
  }
}